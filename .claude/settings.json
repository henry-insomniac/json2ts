{
  "subagents": {
    "enabled": true,
    "default_language": "zh-CN",
    "communication_style": "professional",
    "collaboration_mode": "intelligent",
    
    "agents": {
      "code-explorer": {
        "file": "subagents/code-explorer.md",
        "priority": 1,
        "tools": ["Read", "Glob", "Grep", "LS"],
        "capabilities": [
          "code_structure_analysis",
          "pattern_recognition", 
          "dependency_mapping",
          "issue_identification",
          "improvement_opportunities"
        ],
        "specializations": [
          "Go语言项目分析",
          "架构模式识别",
          "性能瓶颈发现",
          "代码质量评估"
        ]
      },
      
      "planner": {
        "file": "subagents/planner.md", 
        "priority": 2,
        "tools": ["TodoWrite"],
        "capabilities": [
          "requirement_analysis",
          "task_decomposition",
          "resource_planning", 
          "risk_assessment",
          "milestone_definition"
        ],
        "specializations": [
          "技术项目规划",
          "复杂任务分解", 
          "风险管理",
          "时间估算"
        ]
      },
      
      "executor": {
        "file": "subagents/executor.md",
        "priority": 3, 
        "tools": ["Write", "Edit", "MultiEdit", "Bash", "NotebookEdit"],
        "capabilities": [
          "code_implementation",
          "system_integration",
          "testing_execution",
          "build_automation",
          "deployment_management"
        ],
        "specializations": [
          "Go语言开发",
          "自动化脚本",
          "测试集成",
          "构建优化"
        ]
      },
      
      "reviewer": {
        "file": "subagents/reviewer.md",
        "priority": 4,
        "tools": ["Read", "Grep", "Bash"],
        "capabilities": [
          "code_quality_review",
          "security_analysis", 
          "performance_evaluation",
          "best_practices_validation",
          "compliance_checking"
        ],
        "specializations": [
          "Go代码审查",
          "安全漏洞检测",
          "性能分析",
          "架构评估"
        ]
      },
      
      "super-thinking": {
        "file": "subagents/super-thinking.md",
        "priority": 0,
        "tools": ["*"],
        "capabilities": [
          "deep_analysis",
          "creative_problem_solving",
          "system_thinking",
          "innovation_generation",
          "strategic_insight"
        ],
        "specializations": [
          "复杂问题分析",
          "创新方案设计",
          "系统性思维",
          "战略规划"
        ],
        "activation_triggers": [
          "复杂问题",
          "多重解决方案",
          "创新需求", 
          "战略决策",
          "用户明确请求"
        ]
      }
    }
  },
  
  "workflows": {
    "enabled": true,
    "default_workflow": "development",
    
    "available_workflows": {
      "development": {
        "file": "workflows/development.yaml",
        "description": "完整的开发工作流程",
        "stages": ["exploration", "planning", "execution", "review"],
        "estimated_duration": "20-60分钟",
        "suitable_for": [
          "新功能开发",
          "bug修复", 
          "代码重构",
          "性能优化"
        ]
      },
      
      "analysis": {
        "file": "workflows/analysis.yaml", 
        "description": "深度分析和理解工作流",
        "stages": ["initial_exploration", "deep_dive", "synthesis", "validation"],
        "estimated_duration": "15-45分钟",
        "suitable_for": [
          "代码库理解",
          "技术评估",
          "架构分析", 
          "问题诊断"
        ]
      }
    }
  },
  
  "quality_standards": {
    "code_quality": {
      "readability_score": 8.0,
      "maintainability_index": 70,
      "test_coverage_threshold": 80,
      "complexity_limit": 10
    },
    
    "security_requirements": {
      "input_validation": "required",
      "error_handling": "comprehensive", 
      "logging_security": "enabled",
      "dependency_scanning": "automated"
    },
    
    "performance_targets": {
      "response_time": "< 100ms",
      "memory_usage": "< 100MB",
      "cpu_utilization": "< 80%"
    }
  },
  
  "project_context": {
    "name": "json2ts",
    "type": "CLI Tool",
    "language": "Go",
    "version": "1.24.5",
    "purpose": "JSON to TypeScript type generation",
    
    "key_files": [
      "src/cmd/main.go",
      "json/test.json",
      "go.mod"
    ],
    
    "architecture_patterns": [
      "Command-line interface",
      "Type generation",
      "JSON parsing"
    ],
    
    "development_practices": {
      "coding_style": "Go standard",
      "error_handling": "explicit", 
      "testing_framework": "standard testing",
      "documentation": "inline comments"
    }
  },
  
  "collaboration_rules": {
    "cross_agent_communication": {
      "enabled": true,
      "consultation_allowed": true,
      "knowledge_sharing": "encouraged"
    },
    
    "workflow_flexibility": {
      "stage_skipping": "conditional",
      "parallel_execution": "when_possible",
      "adaptive_routing": "enabled"
    },
    
    "quality_gates": {
      "mandatory_review": true,
      "automated_testing": true,
      "security_scanning": true
    }
  },
  
  "user_preferences": {
    "output_language": "zh-CN",
    "detail_level": "comprehensive",
    "explanation_style": "technical_with_examples",
    "progress_reporting": "real_time",
    
    "design_preferences": {
      "ui_style": "Apple官网风格",
      "color_scheme": "柔和对比",
      "layout_pattern": "卡片式布局",
      "responsive_design": true,
      "tech_stack": "React + Tailwind CSS + Radix UI"
    }
  },
  
  "advanced_features": {
    "auto_optimization": {
      "enabled": true,
